{
  "name": "code-challenge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.9.0",
  "author": "anzhang",
  "engines": {
    "node": ">=22.12.0",
    "pnpm": ">=10.9.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true pnpm build",
    "lint": "eslint",
    "prettier:check": "prettier --check \"src//*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:format": "prettier --write \"src//*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint:check": "eslint --max-warnings=0 --no-warn-ignored \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "ts:check": "tsc --noEmit",
    "prepare": "husky",
    "commit": "cz",
    "clean": "rimraf .next out coverage",
    "clean:dev": "pnpm clean && pnpm dev:next"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --max-warnings=0 --no-warn-ignored"
    ]
  },
  "dependencies": {
    "@privy-io/react-auth": "^3.3.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@solana-program/memo": "^0.9.0",
    "@solana-program/system": "^0.9.0",
    "@solana-program/token": "^0.7.0",
    "@solana/kit": "^4.0.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "bignumber.js": "^9.3.1",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "sooner": "^1.1.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@commitlint/types": "^20.0.0",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.6",
    "@tailwindcss/postcss": "^4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "commitizen": "^4.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
